[node name="CEDescription" type="RichTextLabel" parent="BackgroundIMG/ButtonGrid/Adaptation Talents/AdaptationTalents2" groups=["TalentLabel"]]
custom_minimum_size = Vector2(16, 16)
layout_mode = 2
text = " Able to adjust your stance to focus on offense or defense, your ability to adapt is second to none.
–Prerequisites: N/A
-[Triggered/Start of Turn]: Reduce your Awareness or Defense Value by up to 2(bT) to increase your Defense Value or Awareness (respectively) by the reduction until the start of your next turn.
-[Triggered, 1/Round]: When making a Combat Roll as part of an Attacking Maneuver, or in response to an Attacking Maneuver that targets you, gain Greater Dice on that Combat Roll."
fit_content = true

