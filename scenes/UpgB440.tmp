text = "Focused Strike"

[node name="Desc3" type="RichTextLabel" parent="BackgroundIMG/ButtonGrid/Attacking Talents/AttackingTalents" groups=["TalentLabel"]]
custom_minimum_size = Vector2(16, 16)
layout_mode = 2
text = "Able to pinpoint weak points in a target’s defense, your attacks hone in on the least guarded parts of the opponent’s body to land strikes consistently.
–Prerequisites: Artful Strike
-[Passive]: Roll your Base Die twice for any Strike Roll and use the highest result.
-[Passive]: Reduce the Critical Target of your Strike Rolls by 1."
fit_content = true

[node name="MasterfulStrike" type="Button" parent="BackgroundIMG/ButtonGrid/Attacking Talents/AttackingTalents"]
layout_mode = 2
