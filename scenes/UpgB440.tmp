custom_minimum_size = Vector2(16, 16)
layout_mode = 2
text = "Unabashed by dishonorable tactics, you are more than willing to choke the life out of your target.
–Prerequisites: Brawler
-[Passive]: Increase the Wound Roll of your Attacking Maneuvers made against Opponents in a Grapple by 2(T).
-[1/Round]: While you are the Grappler in a Grapple, you can spend 1 Action to target the Grappled and make a Clash (Strike) against them. If you win, they gain the Suffocating Combat Condition until they escape the Grapple."
fit_content = true

[node name="Suplex" type="Button" parent="BackgroundIMG/ButtonGrid/Grappling Talents/GrapplingTalents"]
