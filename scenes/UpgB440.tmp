toggle_mode = true
text = "Masterful Strike"

[node name="Desc4" type="RichTextLabel" parent="BackgroundIMG/ButtonGrid/Attacking Talents/AttackingTalents" groups=["TalentLabel"]]
custom_minimum_size = Vector2(16, 16)
layout_mode = 2
text = "You are able to recover your momentum even when you miss, surprising your opponent with a strike that lands.
–Prerequisites: Artful Strike, Tier of Power 3+
-[Triggered, 1/Round]: If you fail to hit an Opponent with an Attacking Maneuver, reroll your Strike Roll and apply it against the same roll you lost against.
-[1/Encounter]: As an Instant Maneuver you may activate this effect: until the end of your turn, ignore all penalties to your Strike Rolls."
fit_content = true

[node name="PowerfulStrike" type="Button" parent="BackgroundIMG/ButtonGrid/Attacking Talents/AttackingTalents"]
layout_mode = 2
